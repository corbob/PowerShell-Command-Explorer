{
    "name": "powershell-function-explorer",
    "displayName": "PowerShell Function Explorer",
    "description": "Function Explorer for PowerShell",
    "version": "0.0.1",
    "publisher": "CorBob",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:powershell"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "function-explorer",
                    "title": "Function Explorer",
                    "icon": "media/dep.svg"
                }
            ]
        },
        "views": {
            "function-explorer": [
                {
                    "id": "functions",
                    "name": "PowerShell Functions"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
